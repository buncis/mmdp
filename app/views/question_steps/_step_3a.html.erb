<%= simple_form_for(@mataharinya, url: wizard_path, :method => :put) do |f| %>

<%= f.input :dua_variabel_keputusan, label: "Apakah metode peramalan yang akan anda gunakan hanya untuk menyelesaikan permasalahan dengan dua variabel keputusan?
" %>
<%= f.input :linier_programming, label: "Apakah metode peramalan yang akan anda gunakan harus memformulasikan ke dalam bentuk grafik Linier Programming?
" %>
<%= f.input :garis_tren, label: "Apakah metode peramalan yang akan anda gunakan menggunakan garis trend untuk persamaan matematis?
" %>
<%= f.input :garis_lurus, label: "Apakah metode peramalan yang akan anda gunakan menggunakan hanya menarik secara garis lurus sesuai dengan kecenderungan data time series yang ada?
" %>
<div class="form-actions">
  <%= f.button :submit, "Lanjut!" %>
</div>
<% end %>
